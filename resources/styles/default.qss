QWidget {
    color: white;
    background-color: rgb(26, 29, 33);
}

QLabel[type = "header"] {
    color: rgb(61, 66, 77);
}

QLabel[type = "title"] {
    color: white;
    background-color: rgb(61, 66, 77);
}
QLabel[type = "title"]:disabled {
    color: rgb(26, 29, 33);
    background-color: rgb(30, 33, 37);
}

QLabel[type = "gray"] {
    color: gray;
    background-color: rgb(30, 33, 37);
}
QLabel[type = "gray"]:disabled {
    color: rgb(26, 29, 33);
    background-color: rgb(30, 33, 37);
}

QLabel[type = "gray-light"] {
    color: rgb(165, 165, 165);
    background-color: rgb(30, 33, 37);
}
QLabel[type = "gray-light"]:disabled {
    color: rgb(26, 29, 33);
    background-color: rgb(30, 33, 37);
}

QLabel[type = "gray-dark"] {
    color: rgb(61, 66, 77);
    background-color: rgb(30, 33, 37);
}
QLabel[type = "gray-dark"]:disabled {
    color: rgb(26, 29, 33);
    background-color: rgb(30, 33, 37);
}

QTextEdit[type = "source-data"] {
    color: rgb(220, 220, 220);
}
QTextEdit[type = "source-data"]:disabled {
    color: rgb(26, 29, 33);
}

