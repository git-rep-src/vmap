QLineEdit {
    color: white;
    background-color: rgb(30, 33, 37);
    border-style: solid;
    border-width: 1px;
    border-color: rgb(26, 29, 33);
}
QLineEdit:disabled {
    color: rgb(26, 29, 33);
    background-color: rgb(30, 33, 37);
    border-style: solid;
    border-width: 1px;
    border-color: rgb(26, 29, 33);
}

QTextEdit {
    color: white;
    background-color: rgb(30, 33, 37);
}
QTextEdit:disabled {
    color: rgb(26, 29, 33);
    background-color: rgb(30, 33, 37);
}

QComboBox {
    color: white;
    background-color: rgb(61, 66, 77);
    border-style: solid;
    border-width: 1px;
    border-color: rgb(26, 29, 33);
    padding-left: 10px;
    font-size: 15px;
}
QComboBox:disabled {
    color: rgb(26, 29, 33);
    background-color: rgb(30, 33, 37);
    border-style: solid;
    border-width: 1px;
    border-color: rgb(26, 29, 33);
    padding-left: 10px;
    font-size: 15px;
}
QComboBox::drop-down {
    color: white;
    background-color: rgb(61, 66, 77);
    border-style: solid;
    border-width: 1px;
    border-color: rgb(61, 66, 77);
}
QComboBox::drop-down:disabled {
    color: rgb(26, 29, 33);
    background-color: rgb(30, 33, 37);
    border-style: solid;
    border-width: 1px;
    border-color: rgb(30, 33, 37);
}
/*
QComboBox::down-arrow {
    image: url(":/icon-open-disabled");
    width: 16px;
    height: 16px;
}
*/
QComboBox QAbstractItemView::item {
    color: white;
    background-color: rgb(30, 33, 37);
    border-style: solid;
    border-width: 1px;
    border-color: rgb(26, 29, 33);
    min-height: 29px;
    padding-left: 10px;
}
QListView::item:selected {
    color: white;
    background-color: rgb(165, 165, 165);
    padding-left: 10px;
}

QPushButton {
    color: white;
    background-color: rgb(18, 21, 25);
}
QPushButton:disabled {
    color: rgb(61, 66, 77);
    background-color: rgb(18, 21, 25);
}



QToolTip {
    color: white;
    background-color: rgb(26, 29, 33);
}
